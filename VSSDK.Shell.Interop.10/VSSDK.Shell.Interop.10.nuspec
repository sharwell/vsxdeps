<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.Shell.Interop.10</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "Shell" COM-interop assemblies used by Visual Studio 2010 and newer.

      Microsoft.VisualStudio.Shell.Interop.10.0.dll
    </description>
    <summary>Visual Studio (Shell Interop) for Visual Studio 2010+</summary>
    <tags>vssdk vs2010 vs2012 vs2013 vs2015</tags>
    <dependencies>
      <!-- Visual Studio 2010 -->
      <dependency id="VSSDK.IDE.10" version="[10.0.4,11.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Shell.Interop.10.0.dll):
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.Interop
        Microsoft.VisualStudio.Shell.Interop.8.0
      -->
      <dependency id="VSSDK.OLE.Interop" version="[7.0.4,8.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.8" version="[8.0.4,9.0.0)"/>
      <!-- Also reference version from 2008 -->
      <dependency id="VSSDK.Shell.Interop.9" version="[9.0.4,10.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="Microsoft.VisualStudio.Shell.Interop.10.0.dll" target="lib\net20"/>
    <file src="Microsoft.VisualStudio.Shell.Interop.10.0.xml" target="lib\net20"/>
  </files>
</package>
